
#include <iostream>
using namespace std;

int main() {
	int T;
	cin>>T;
	for(int i=1;i<=T;i++){
	    string d;
	    int q,n;
	    cin>>n;
	    cin>>d;
	    q=0;
	  for(int i=0;i<n;i++)
	  {
       if(d[i]=='0'|| d[i]=='5')
	  {
	            q++;
	  }
	 }
	    if(q>0){
	        cout<<"Yes"<<endl;
	    }
	    else{
	        cout<<"No"<<endl;
	    }
	}
	return 0;
}
